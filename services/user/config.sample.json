    "base_url": "/v1/service_urls_base_url",
    "heart_beat": {
      "url": "/health",
      "ttl": 5
    }
  },
  "server": {
    "port": 8080,
    "otp_ttl_minutes": 5,
    "password_secret": "your_password_secret_key",
    "secret": "your_jwt_secret_key",
    "auth_expiration_minutes": 60,
    "auth_refresh_minutes": 1440
  },
  "redis": {
    "host": "golibaba-redis",
    "port": 6379
  }
}