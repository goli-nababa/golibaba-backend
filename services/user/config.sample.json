{
  "db": {
    "host": "golibaba-db",
    "port": 5432,
    "user": "porestgres",
    "pass": "postgres",
    "name": "postgres",
    "schema": "public"
  },
  "services": {
    "gateway": "http://golibaba-gateway-app:8080"
  },
  "service_info": {
    "name": "sample_service",
    "version": "1.0.0",
